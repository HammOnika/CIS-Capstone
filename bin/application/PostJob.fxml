<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main">
  <children>
      <HBox fillHeight="false" prefHeight="111.0" prefWidth="640.0">
         <children>
            <Button fx:id="showLogin1" mnemonicParsing="false" onAction="#showLogin" text="Sign out">
               <HBox.margin>
                  <Insets left="25.0" right="25.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="showMessaging" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#showMessaging" text="Inbox">
               <HBox.margin>
                  <Insets right="25.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="showStudentHome" layoutX="161.0" layoutY="10.0" mnemonicParsing="false" onAction="#showRecruiterHome" text="Home">
               <HBox.margin>
                  <Insets right="25.0" />
               </HBox.margin>
            </Button>
         </children>
         <padding>
            <Insets right="25.0" />
         </padding>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="102.0" prefWidth="640.0" text="Create Job Listing" />
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Job Title" />
            <TextField fx:id="username" prefHeight="65.0" prefWidth="506.0" />
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="110.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="85.0" prefWidth="210.0" text="Job Description" />
            <TextField fx:id="password" prefHeight="100.0" prefWidth="427.0" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="80.0" prefWidth="364.0" text="General Requirments" />
            <TextField fx:id="password1" prefHeight="100.0" prefWidth="427.0" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="showLogin" mnemonicParsing="false" onAction="#login" text="Post" />
         </children>
      </HBox>
  </children>
</VBox>
