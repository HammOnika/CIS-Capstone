<VBox prefHeight="400.0" prefWidth="640.0" stylesheets="application.application" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main">
  <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Login" />
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Username     " />
            <TextField fx:id="username" prefHeight="65.0" prefWidth="407.0" />
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="110.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Password      " />
            <TextField fx:id="password" prefHeight="65.0" prefWidth="410.0" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="50.0">
         <children>
            <Button fx:id="showLogin" mnemonicParsing="false" onAction="#login" text="Login" />
            <Button fx:id="showRegistration" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#showRegistration" text="Register" />
         </children>
         <padding>
            <Insets left="25.0" right="25.0" />
         </padding>
      </HBox>
  </children>
</VBox>
